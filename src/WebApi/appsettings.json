      "Serilog.Extensions.Logging",
      "Serilog.Enrichers.Environment",
      "Serilog.Settings.Configuration",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Elasticsearch",
      "Sentry.Serilog"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
