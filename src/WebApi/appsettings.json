        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "trine-api-index-{0:yyyy.MM}",
          "autoRegisterTemplate": true
        }
      },
      {
        "Name": "Sentry"
      }
    ],
    "Enrich": [
